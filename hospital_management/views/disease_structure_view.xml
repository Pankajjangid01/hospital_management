<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="diseases_structure_form" model="ir.ui.view">
        <field name="name">diseases.structure.form</field>
        <field name="model">diseases.structure</field>
        <field name="arch" type="xml">
            <form string="Diseases Category Form">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="disease_structure_list" model="ir.ui.view">
        <field name="name">diseases.structure.list</field>
        <field name="model">diseases.structure</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>
    <record id="diseases_structure_actions" model="ir.actions.act_window">
        <field name="name">Diseases Category Data</field>
        <field name="res_model">diseases.structure</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>