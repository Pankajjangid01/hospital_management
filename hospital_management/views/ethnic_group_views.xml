<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="ethnic_group_form" model="ir.ui.view">
        <field name="name">ethnic.group.form</field>
        <field name="model">ethnic.group</field>
        <field name="arch" type="xml">
            <form string="Ethnic Group Form">
                <sheet>
                    <group>
                        <field name="ethnic_group"/>
                        <field name="code"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="ethnic_group_list" model="ir.ui.view">
        <field name="name">ethnic.group.list</field>
        <field name="model">ethnic.group</field>
        <field name="arch" type="xml">
            <list>
                <field name="ethnic_group"/>
                <field name="code"/>
            </list>
        </field>
    </record>

    <record id="ethnic_group_actions" model="ir.actions.act_window">
        <field name="name">Ethnic Group Data</field>
        <field name="res_model">ethnic.group</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
